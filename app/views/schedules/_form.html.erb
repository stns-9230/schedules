<div class="jumbotron">
  <%= form_with(model: @schedule, local: true) do |f| %>
    <%= render 'layouts/error_messages', model: f.object %>
    <div class="form-group">
    <%= f.label :title, 'タイトル' %>
    <%= f.text_field :title, class: 'form-group' %>
    </div>
  
    <div class="form-group">
      <%= f.label :start, '開始時刻' %>
      <%= f.datetime_select :start %>
    </div>
  
    <div class="form-group">
      <%= f.label :end, '終了時刻' %>
      <%= f.datetime_select :end %>
    </div>
  
    <div class="form-group">
      <%= f.label :content, '予定詳細' %>
      <%= f.text_area :content, class: 'form-control' %>
    </div>
    <%= f.submit '予定投稿', class: 'btn btn-primary btn-sm' %>
  <% end %>
</div>